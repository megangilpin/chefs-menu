{
    "name": "server",
    "version": "0.0.0",
    "private": true,
    "engines": {
        "node": "12.x"
    },
    "scripts": {
        "start": "node app.js",
        "dev": "nodemon ./bin/www",
        "debug": "nodemon  --inspect ./bin/www",
        "seed": "node scripts/seedDB.js",
        "client": "cd client && npm run start",
