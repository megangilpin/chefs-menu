        "build": "cd client && npm run build",
        "heroku-postbuild": "cd client && npm run build",
        "test": "NODE_ENV=test ATLAS_URI=mongodb://localhost:27017/corn-pops-test ./node_modules/.bin/mocha --exit --timeout 30000"
    },
    "dependencies": {
        "aws-sdk": "^2.794.0",
        "axios": "^0.21.0",
        "bcrypt": "^5.0.0",
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "dotenv": "^8.2.0",
        "express": "~4.16.1",
        "http-errors": "~1.6.3",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.10.13",
        "morgan": "~1.9.1",
        "multer": "^1.4.2",
        "multer-s3": "^2.9.0",
        "stripe": "^8.127.0"
    },
    "devDependencies": {
        "nodemon": "^1.19.1",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "mocha": "^6.2.0"
    }
